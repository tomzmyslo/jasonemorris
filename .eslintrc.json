{
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off"
    }
}
